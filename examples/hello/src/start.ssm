@section "WRAM0"

stackTop: @ds 128
stackBase:

@section "BANK0"

Start::
    di
    ld sp, stackBase

    ld de, $8000
    ld hl, LetterH
    ld bc, 16
    call MemCopy

.Wait:
    halt
    jp .Wait

